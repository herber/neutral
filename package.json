{
  "name": "neutral",
  "version": "0.0.3",
  "description": "Minimalist frontend framework",
  "license": "MIT",
  "repository": "herber/neutral",
  "source": "lib/index.js",
  "main": "dist/neutral.js",
  "module": "dist/neutral.m.js",
  "browser": "dist/neutral.umd.js",
  "author": {
    "name": "Tobias Herber",
    "email": "tobihrbr@gmail.com",
    "url": "tobihrbr.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "formatted && npm run build && jest",
    "build": "rollup -c"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "frontend",
    "framework"
  ],
  "dependencies": {
    "document-ready": "^2.0.1",
    "mitt": "^1.1.3",
    "nanohref": "^3.0.1",
    "nation": "^1.2.0",
    "route-changed": "^1.0.2",
    "shipharbor": "^1.0.0",
    "xou": "^2.0.2"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "codecov": "^3.0.0",
    "formatted": "^1.0.0",
    "jest": "^22.4.2",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "vxv": "^3.1.6"
  },
  "formatted": {
    "ignore": [
      "**/node_modules/**",
      "**/dist/**"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testMatch": [
      "**/test/**"
    ]
  }
}
